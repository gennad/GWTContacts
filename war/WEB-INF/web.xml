<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <servlet>
    <servlet-name>loaderServlet</servlet-name>
    <servlet-class>ru.duonox.extcontacts.server.LoaderImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>loaderServlet</servlet-name>
    <url-pattern>/extended_contacts/loader</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>Extended_Contacts.html</welcome-file>
  </welcome-file-list>

</web-app>